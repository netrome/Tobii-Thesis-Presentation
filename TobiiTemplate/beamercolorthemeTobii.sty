%%Colour theme for Tobii


\definecolor{Primary}{RGB}{2,22,31}
\definecolor{Secondary}{RGB}{52,55,65}
\definecolor{Tertiary}{RGB}{0, 190, 255}
\definecolor{CoolGray}{RGB}{65,64,66}
\definecolor{LightGray}{RGB}{188,190,192}

%\setbeamercolor*{title page header}{bg=white}

\setbeamercolor*{structure}{fg=black}
\setbeamercolor*{frametitle}{fg=black}
\setbeamercolor*{framesubtitle}{fg=red}
\setbeamercolor*{footline}{fg=Secondary}

\setbeamercolor*{palette primary}{use=structure,fg=white,bg=structure.fg}
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg=structure.fg!75!black}
\setbeamercolor*{palette tertiary}{use=structure,fg=white,bg=structure.fg!50!black}
\setbeamercolor*{palette quaternary}{fg=white,bg=black}

\setbeamercolor*{sidebar}{use=structure,bg=structure.fg}
  
\setbeamercolor*{palette sidebar primary}{use=structure,fg=structure.fg!10}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=structure.fg!50}
\setbeamercolor*{palette sidebar quaternary}{fg=white}

\setbeamercolor*{titlelike}{fg=white}

\setbeamercolor{title}{parent=titlelike}
\setbeamercolor{subtitle}{parent=titlelike}
\setbeamercolor{author}{parent=titlelike}
\setbeamercolor{date}{parent=titlelike}
\setbeamercolor{title in head/foot}{parent=palette quaternary}
\setbeamercolor{title in sidebar}{parent=palette sidebar quaternary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamercolor{block body}{fg=black}
\setbeamercolor{block body alerted}{}
\setbeamercolor{block body example}{}
\setbeamercolor{block title}{parent=structure}
\setbeamercolor{block title alerted}{parent=alerted text}
\setbeamercolor{block title example}{parent=example text}

\setbeamercolor{item}{fg=structure.fg!30!black}
\setbeamercolor{section in toc}{fg=fg=structure.fg!30!Black}
\mode
<all>
